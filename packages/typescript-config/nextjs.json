{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": ["./base.json", "./react.json"],
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true
  }
}
